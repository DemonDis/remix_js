{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"npm run dev:sass\" \"remix dev\"",
    "build:css": "node-sass ./styles -o ./app/styles --output-style compressed",
    "start": "concurrently \"npm run dev:sass\" \"remix dev\"",
    "start:sass": "npm run build:css && node-sass -rw ./styles -o ./app/styles"
  },
  "dependencies": {
    "@remix-run/node": "^1.10.1",
    "@remix-run/react": "^1.10.1",
    "@remix-run/serve": "^1.10.1",
    "isbot": "^3.6.5",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.10.1",
    "@remix-run/eslint-config": "^1.10.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
